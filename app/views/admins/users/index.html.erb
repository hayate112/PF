<h2>会員一覧</h2>

<table>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td>
      <%= link_to admins_user_path(user.id) do %>
        <%= user.family_name %><%= user.name %>
      <% end %>
    </td>
    <td><%= user.email %></td>
    <td>
      <% if user.is_deleted == false %>
        有効
      <% else %>
        退会
      <% end %>
    </td>
  </tr>
  <% end %>
</table>
<%= paginate @users %>